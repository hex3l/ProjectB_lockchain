{
  "name": "boilerplate-app",
  "version": "0.0.0",
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "next dev --port 3100",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "tsc": "tsc -p tsconfig.json",
    "format-fix": "prettier --config ../../.prettierrc --write --ignore-unknown -l .",
    "test": "jest --verbose",
    "test-watch": "jest --verbose --watch",
    "test-coverage": "jest --coverage --verbose",
    "storybook": "storybook dev -p 6100 --quiet",
    "storybook-build": "storybook build -o build-storybook-static",
    "update-dependencies": "npx npm-check-updates -u"
  },
  "dependencies": {
    "config-mui": "*",
    "config-tailwind": "*",
    "notistack": "^3.0.1",
    "react-parallax-tilt": "1.7.170",
    "react-slideshow-image": "^4.3.0",
    "trpc-api-boilerplate": "1.0.15",
    "ui": "*",
    "utils-mui": "*",
    "utils-version": "*",
    "viem": "^1.19.3",
    "wagmi": "^1.4.7"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.0.2",
    "@tanstack/eslint-plugin-query": "4.36.1",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "config-storybook": "*",
    "config-tsconfig": "*",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-no-array-reduce": "1.0.62",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-testing-library": "6.0.2",
    "utils-test": "*"
  }
}
